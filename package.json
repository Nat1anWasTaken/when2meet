{
    "name": "when2meet",
    "private": true,
    "version": "0.0.1",
    "license": "AGPL-3.0",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "prepare": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && svelte-kit sync || echo ''",
        "check": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "db:push": "drizzle-kit push",
        "db:migrate": "drizzle-kit migrate",
        "db:studio": "drizzle-kit studio"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.5",
        "@eslint/js": "^9.18.0",
        "@iconify/json": "^2.2.377",
        "@inlang/cli": "^3.0.12",
        "@internationalized/date": "^3.8.2",
        "@lucide/svelte": "^0.515.0",
        "@sveltejs/adapter-auto": "^6.0.0",
        "@sveltejs/adapter-vercel": "^5.10.2",
        "@sveltejs/kit": "^2.27.0",
        "@sveltejs/vite-plugin-svelte": "^6.0.0",
        "@tailwindcss/typography": "^0.5.15",
        "@tailwindcss/vite": "^4.0.0",
        "@types/node": "^22",
        "bits-ui": "^2.9.1",
        "clsx": "^2.1.1",
        "drizzle-kit": "^0.30.2",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-formatter-unix": "^8.40.0",
        "eslint-plugin-svelte": "^3.0.0",
        "globals": "^16.0.0",
        "prettier": "^3.4.2",
        "prettier-plugin-svelte": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "svelte": "^5.0.0",
        "svelte-check": "^4.0.0",
        "svelte-sonner": "^1.0.5",
        "tailwind-merge": "^3.3.1",
        "tailwind-variants": "^1.0.0",
        "tailwindcss": "^4.0.0",
        "tsx": "^4.20.3",
        "tw-animate-css": "^1.3.6",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.20.0",
        "unplugin-icons": "^22.2.0",
        "vite": "^7.0.4"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    },
    "dependencies": {
        "@inlang/paraglide-js": "^2.0.0",
        "@prisma/client": "^6.13.0",
        "@takumi-rs/core": "^0.51.1",
        "@takumi-rs/helpers": "^0.51.1",
        "@takumi-rs/image-response": "^0.51.1",
        "better-auth": "^1.3.4",
        "drizzle-orm": "^0.40.0",
        "drizzle-zod": "^0.8.3",
        "mode-watcher": "^1.1.0",
        "postgres": "^3.4.5",
        "prisma-zod-generator": "^1.1.1",
        "zod": "^4.0.15"
    }
}
